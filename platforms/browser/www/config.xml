<?xml version='1.0' encoding='utf-8'?>
<widget id="TwojaFormarma" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>TwojaForma</name>
    <description>My first mobile to control your body</description>
    <author email="marcin.feliks91@gmail.com" href="marcin.feliks.com">
		Marcin Feliks
	</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <splash src="img/splash.png" />
    <icon src="img/icon.png" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
